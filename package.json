{
  "name": "@sequra/mufasa-js-sdk",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/sequra"
  },
  "version": "1.0.0",
  "description": "A library to integrate the Mufasa iframe",
  "main": "dist/index.js",
  "types": "src/types.d.ts",
  "scripts": {
    "dev": "webpack --watch --config webpack.dev.js",
    "dist": "webpack --config webpack.prod.js",
    "test": "jest --verbose",
    "test:ci": "jest --ci --colors --silent --reporters=default --reporters=jest-junit",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "prepare": "webpack --config webpack.package.js"
  },
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sequra/mufasa-js-sdk.git"
  },
  "keywords": [
    "pci",
    "mufasa",
    "iframe"
  ],
  "author": "SeQura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sequra/mufasa-js-sdk/issues"
  },
  "homepage": "https://github.com/sequra/mufasa-js-sdk#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "esbuild": "^0.14.23",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.18.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom-global": "^3.0.0",
    "jest-junit": "^13.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
